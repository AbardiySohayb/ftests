
@{
    ViewData["Title"] = "Ens";
}

<h1>Ens</h1>

@model ftest.Models.AjoutEtdProfViewModel

@using (Html.BeginForm("Ens", "Etd", FormMethod.Post))
{
    <div class="form-group">
        <label for="EtdId">Sélectionnez un étudiant :</label>
        @Html.DropDownListFor(model => model.EtdId, new SelectList(Model.Etudiants, "EtdId", "Nom"), "Sélectionnez un étudiant", new { @class = "form-control" })
    </div>

    <div class="form-group">
        <label for="ProfId">Sélectionnez un professeur :</label>
        @Html.DropDownListFor(model => model.ProfId, new SelectList(Model.Professeurs, "ProfId", "Nom"), "Sélectionnez un professeur", new { @class = "form-control" })
    </div>

    <button type="submit" class="btn btn-primary">Ajouter</button>
}

